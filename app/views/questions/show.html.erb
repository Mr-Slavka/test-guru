
  <h1>Question</h1>
  <%= @question.inspect %><br>


  <h2>Answers</h2>
  <%= link_to 'New answer', new_question_answer_path(@question) %>

  <ol>
    <%= render @question.answers %>
  </ol>

  <p> <%= link_to 'Back to questions', test_path(@question.test) %> </p>
  <p> <%= link_to "Delete?", @question, method: :delete,data: { confirm: 'Are you sure?' } %> </p>