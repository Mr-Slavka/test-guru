
  <h2>Create a new question for test : <%= @test.title %></h2>
  <form action="/tests/<%= @test.id %>/questions" method="post">
    <input  value="<%= form_authenticity_token %>" type="hidden" name="authenticity_token">
    <p>
      <label for="question-body">Question:</label>
      <input type="text" name="question[body]" id="question-body">
    </p><br>

    <input type="submit" value="Create">
  </form>
